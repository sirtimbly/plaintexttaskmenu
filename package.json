  "main": "index.js",
  "dependencies": {
    "commander": "^2.3.0",
    "js-yaml": "^3.2.2",
    "lodash": "^2.4.1",
    "marked": "^0.3.5",
    "menubar": "*",
    "moment": "^2.10.6",
    "walk": "*"
  },
  "devDependencies": {
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.36.0"
  },
  "engines": {
    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sirtimbly/manuscript-logger.git"
  },
  "preferGlobal": true,
  "bin": {
    "task-scheduler": "index.js"
